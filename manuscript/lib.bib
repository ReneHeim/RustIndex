	journal = {Remote Sensing Applications: Society and Environment},
	author = {Chicati, Marcelo Luiz and Nanni, Marcos Rafael and Cezar, Everson and de Oliveira, Roney Berti and Chicati, Mônica Sacioto},
	month = apr,
	year = {2017},
	keywords = {Floodplains, Soil classification, Spectral reflectance},
