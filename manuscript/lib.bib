	keywords = {Hyperspectral data, Spectral features, Wavelet transforms, Winter wheat, Yellow rust},
	pages = {79--87},
	file = {ScienceDirect Snapshot:D\:\\Zotero\\storage\\5UNVBNZ5\\S0168169913002652.html:text/html;Zhang et al. - 2014 - Comparison between wavelet spectral features and c.pdf:D\:\\Zotero\\storage\\FMNU4JVZ\\Zhang et al. - 2014 - Comparison between wavelet spectral features and c.pdf:application/pdf}
}

