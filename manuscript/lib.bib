	pages = {62--67},
	file = {IEEE Xplore Abstract Record:D\:\\Zotero\\storage\\3E99TCGG\\abs_all.html:text/html;IEEE Xplore Full Text PDF:D\:\\Zotero\\storage\\XSRK9JVE\\Burkart et al. - 2014 - A Novel UAV-Based Ultra-Light Weight Spectrometer .pdf:application/pdf}
