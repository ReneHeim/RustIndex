	pages = {177--190},
	file = {Full Text PDF:D\:\\Zotero\\storage\\GUZZNB4T\\Jucker et al. - 2017 - Allometric equations for integrating remote sensin.pdf:application/pdf;Snapshot:D\:\\Zotero\\storage\\6TTEVE35\\abstract.html:text/html}
