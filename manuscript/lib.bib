	pmid = {21669634},
	keywords = {matK;, molecular phylogeny, Morphology, Myrtaceae},
	pages = {2013--2025},
	file = {Full Text PDF:D\:\\Zotero\\storage\\3FNK7TJT\\Wilson et al. - 2001 - Myrtaceae revisited a reassessment of infrafamili.pdf:application/pdf;Snapshot:D\:\\Zotero\\storage\\AJTXV6H7\\2013.html:text/html}
