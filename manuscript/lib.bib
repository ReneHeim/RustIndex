@article{brewer_functional_1991,
	title = {Functional interaction between leaf trichomes, leaf wettability and the optical properties of water droplets},
	volume = {14},
	issn = {1365-3040},
	url = {http://onlinelibrary.wiley.com.simsrad.net.ocs.mq.edu.au/doi/10.1111/j.1365-3040.1991.tb00965.x/abstract},
	doi = {10.1111/j.1365-3040.1991.tb00965.x},
	abstract = {Abstract. Because CO2 diffuses 10000 times more slowly through water than air, there may be strong selective pressure for increased water repellency in terrestrial plant leaves. In the present study, leaf trichomes appeared to have a strong influence on leaf water repellency (i.e. degree of water droplet formation on the leaf surface) as well as the retention of droplets on the leaf. Based upon evaluation of 38 plant species from 21 families, we found that leaves with trichomes were more water repellent, especially where trichome density was greater than 25mm2. However, droplet repellency and retention were both high in some species where trichomes entrapped droplets. Finally, the lensing effects of water droplets on leaf surfaces increased incident sunlight by over 20-fold directly beneath individual droplets. These results may have important implications for such processes as stomatal function, whole leaf photosynthesis, and transpiration for a large variety of plant species.},
	language = {en},
	number = {9},
	urldate = {2017-04-02},
	journal = {Plant, Cell \& Environment},
	author = {Brewer, C. A. and Smith, W. K. and Vogelmann, T. C.},
	month = dec,
	year = {1991},
	keywords = {droplet optics, droplet retention, leaf wettability, Trichomes, water droplets},
	pages = {955--962},
	file = {Brewer et al_1991_Functional interaction between leaf trichomes, leaf wettability and the optical.pdf:D\:\\Zotero\\storage\\ZI4V5UH3\\Brewer et al_1991_Functional interaction between leaf trichomes, leaf wettability and the optical.pdf:application/pdf;Snapshot:D\:\\Zotero\\storage\\B2J6XQJT\\abstract.html:text/html}
}

