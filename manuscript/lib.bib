	pages = {1778--1790},
	file = {IEEE Xplore Abstract Record:D\:\\Zotero\\storage\\VITXXDZW\\1323134.html:text/html;Melgani_Bruzzone_2004_Classification of hyperspectral remote sensing images with support vector.pdf:D\:\\Zotero\\storage\\527FAW7J\\Melgani_Bruzzone_2004_Classification of hyperspectral remote sensing images with support vector.pdf:application/pdf}
