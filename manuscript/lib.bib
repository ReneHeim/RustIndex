	pages = {261--270},
	file = {IEEE Xplore Abstract Record:D\:\\Zotero\\storage\\NK2NANHU\\134076.html:text/html;Kaufman_Tanre_1992_Atmospherically resistant vegetation index (ARVI) for EOS-MODIS.pdf:D\:\\Zotero\\storage\\PETI6KZE\\Kaufman_Tanre_1992_Atmospherically resistant vegetation index (ARVI) for EOS-MODIS.pdf:application/pdf}
