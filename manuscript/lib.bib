	file = {Full Text PDF:D\:\\Zotero\\storage\\E9I2DJEZ\\Prabhu et al. - 2016 - Wavelet Based Feature Extraction Techniques of Hyp.pdf:application/pdf;Snapshot:D\:\\Zotero\\storage\\2BTMHDT7\\s12524-015-0506-9.html:text/html}
}

