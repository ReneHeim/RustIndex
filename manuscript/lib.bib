	journal = {BioScience},
	author = {Ustin, Susan L. and Roberts, Dar A. and Gamon, John A. and Asner, Gregory P. and Green, Robert O.},
	year = {2004},
	keywords = {hyperspectral imaging, Imaging spectroscopy, landscape ecology, Remote sensing, spectral analysis},
	pages = {523--534},
