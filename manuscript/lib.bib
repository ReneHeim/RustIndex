@article{gitelson_signature_1996,
	title = {Signature {Analysis} of {Leaf} {Reflectance} {Spectra}: {Algorithm} {Development} for {Remote} {Sensing} of {Chlorophyll}},
	volume = {148},
	issn = {0176-1617},
	shorttitle = {Signature {Analysis} of {Leaf} {Reflectance} {Spectra}},
	url = {http://www.sciencedirect.com/science/article/pii/S0176161796802847},
	doi = {10.1016/S0176-1617(96)80284-7},
	abstract = {Summary
The goal of the study is to investigate the basic spectral properties of plant leaves to develop spectral indices more sensitive to chlorophyll concentration than the presently widely used Notmalized Difference Vegetation Index. These indices can serve as indicators of stress, senescence, and disease in higher plants. The spectral reflectance of senescing leaves of two deciduous species (maple and chestnut) as well as their pigment content were measured. Spectral indices were developed using reflectances corresponding to wavelengths with maximum and minimum sensitivity to variation in pigment concentration. The signature analysis of reflectance spectra indicated that, for a wide range of leaf greenness (completely yellow to dark green leaves), the maximum sensitivity of reflectance coincides with the maximum absorption of chlorophyll a at 670 nm. However, for yellow-green to green leaves (minimum chlorophyll a as low as 3-5 nmol/cm2), the reflectance near 670nm is not sensitive to chlorophyll concentration due to saturation effects. Therefore, it seems inappropriate to use this spectral band for pigment estimation in yellow-green to green vegetation. The spectral bands ranging from 400 to 480 nm and above 730 nm are not sensitive to chlorophyll concentration as found for 670 nm. The reflectances at these wavelengths could be used as references in the vegetation indices. Maximum sensitivity to chlorophyll a concentration was found at 550-560 nm and 700-710 nm. Reflectances at 700 nm correlated very well with that at 550 nm for a wide range of chlorophyll concentrations for both plant species studied. The inverse reflectance, R550)-1 and (R700)-1 are proportional to chlorophyll a concentration; therefore indices R750/R550 and R750/R700 are directly proportional (correlation r2 \&gt; 0.95) to chlorophyll concentration. These indices were tested for a wide range of chlorophyll a concentration, using several independent data sets. The estimation error in the derivation of chlorophyll concentration from the indices is assessed to be less than 1.2 nmol/cm2.},
	number = {3–4},
	urldate = {2016-04-21},
	journal = {Journal of Plant Physiology},
	author = {Gitelson, Anatoly A. and Merzlyak, Mark N.},
	year = {1996},
	keywords = {Reflectance spectra of leaves, Remote sensing, vegetation indices},
	pages = {494--500},
	file = {Gitelson and Merzlyak - 1996 - Signature Analysis of Leaf Reflectance Spectra Al.pdf:D\:\\Zotero\\storage\\B38BFZZQ\\Gitelson and Merzlyak - 1996 - Signature Analysis of Leaf Reflectance Spectra Al.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zotero\\storage\\8APQZHAW\\S0176161796802847.html:text/html}
}

@article{grant_diffuse_1987,
	title = {Diffuse and specular characteristics of leaf reflectance},
	volume = {22},
	issn = {0034-4257},
	url = {http://www.sciencedirect.com/science/article/pii/0034425787900642},
	doi = {10.1016/0034-4257(87)90064-2},
	abstract = {Leaves are neither purely diffuse nor purely specular reflectors. Instead, they have both diffuse and specular characteristics. The diffuse, Lambertian character of leaf reflectance emanates primarily from the interior of the leaf through multiple scattering, although there may be a small contribution of scattering from rough elements at the leaf surface. The specular and non-Lambertian character of leaf reflectance arises at the surface of the leaf. In this paper, the evolution of our current understanding of the mechanisms of leaf reflectance is reviewed. The use of measurements of polarized reflectance to separate leaf reflectance into diffuse and specular components is discussed. A section on the factors influencing leaf reflectance-leaf structure and physiological disturbances-is included along with discussion on the manner in which these influences are manifest.},
	number = {2},
	urldate = {2016-04-21},
